#r=int(input("How many Pseudoprime "))
def isComposite(x):
    f=0
    for i in range(2,(x//2)+1):
        if x%i==0:
            f=1
            break
    if f==1:
        return True
    else:
        return False                 
def Femat_Psedo_Prime(Start_Limit,End_Limit):
    p,r=Start_Limit,End_Limit
    if p==1:
        p=2
    x=2
    while(r>0):
        k=0     
        while(k==0):
            a=(pow(p,(x-1))-1)%x
            if a==0:
                if(isComposite(x)):
                    print(x)
                    k=1
                x=x+1
            
            else:
                x=x+1
        r=r-1
#12
def Pythagorean_Prime(Start_Limit,End_Limit):
    p,r=Start_Limit,End_Limit
    if p==1:
        p=2
    for a in range(p,r+1):
        k=0     
        for i in range(2,int(a/2)+1): 
            if(a%i==0):
                k=k+1     
                break
        if(k<=0):         
            p=a
            p-=1
            p/=4
            if(p-int(p)==0):
                print(a)
#13
def Reversible_Prime(Start_Limit,End_Limit):
    x=Start_Limit
    y=End_Limit
    if x<12:
        x=12
    for a in range(x,y+1):
        k=0  
        sum=0
        for i in range(2,int(a/2)+1): 
            if(a%i==0):
                k=k+1     
                break
        if(k<=0):         
            
            num=a
            q=0
            p=[]
            while(num>0):
                rem=num%10
                p.append(rem)
                num=int(num/10)
                q+=1
                m=0
            while(q>0):
                sum=sum+p[m]*(10**q)
                m+=1
                q-=1
            sum=int(sum/10)
            k=0     
        for i in range(2,int(sum/2)+1): 
            if(sum%i==0):
                k=k+1     
                break
        if(k<=0):
            print(a)
#14
def Semi_Prime(p,r):
    for a in range(p,r+1):
        j=a
        n=a
        x=[]
        while(n!=1):
            for i in range(2,int(n+1)):
                a=n/i
                p=a-int(a)
                if p!=0 :
                    continue
                else:
                    x.append(i)
                    n=a
                    break
        if (len(x)==2):
            print(j)
#15
def prime(n):    
    for i in range(2,int(n/2)+1): 
        if(n%i==0):
            return 0
    return 1
def SophieGermain_Prime(p,r):
    if p==1:
        p=2
    for a in range(p,r+1):
        k=0     
        for i in range(2,int(a/2)+1): 
            if(a%i==0):
                k=k+1     
                break
        if(k<=0):  
            x=prime(2*a+1)
            if(x==1):
                print(a)
#16
def Twin_Prime(p,r):
    if p==1:
        p=2
        x=''
        y=''
    for a in range(p,r+1):
        k=0     
        for i in range(2,int(a/2)+1): 
            if(a%i==0):
                k=k+1     
                break
        if(k<=0):  
            if(x==''):
                x=int(a)
                continue
            if(y==''):
                y=int(a)
                if((y-x)==2):
                    print('(',x,',',y,')')
            else:
                x=y
                y=a
                if((y-x)==2):
                    print('(',x,',',y,')')
#17
def Wagstaff_Prime(p,r):
    import math
    if p==1:
        p=2
    for a in range(p,r+1):
        k=0     
        for i in range(2,int(a/2)+1): 
            if(a%i==0):
                k=k+1     
                break
        if(k<=0): 
            x=a        
            a*=3
            a-=1
            a=math.log2(a)
            if(a-int(a)==0):
                print(x)
#18
def Wieferich_Prime(p,r):
    from fractions import Fraction
    if p==1:
        p=2
    for a in range(p,r+1):
        k=0     
        for i in range(2,int(a/2)+1): 
            if(a%i==0):
                k=k+1     
                break
        if(k<=0):  
            x=a*a
            y=2**(a-1)-1
            z=Fraction(y,x)
            z=(z)-int(z)
            if(z==0.0):
                print(a)    
#19
def Wilson_Prime(p,r):
    from fractions import Fraction
    import math
    if p==1:
        p=2
    for a in range(p,r+1):
        k=0     
        for i in range(2,int(a/2)+1): 
            if(a%i==0):
                k=k+1     
                break
        if(k<=0):  
            x=a*a
            y=math.factorial(a-1)+1
            z=Fraction(y,x)
            z=(z)-int(z)
            if(z==0.0):
                print(a)
#20
def check_Prime(a):
    k=0
    if a==1:
        return 0
    for i in range(2,int(a/2)+1): 
        if(a%i==0):
            k=k+1     
            break
    if(k<=0):         
        return 1
    else:
        return 0
def check_Not_Prime(a):
    k=0
    if a==1:
        return 1
    for i in range(2,int(a/2)+1): 
        if(a%i==0):
            k=k+1     
            break
    if(k<=0):         
        return 0
    else:
        return 1
def left_Tru(t,r):
    for a in range(t,r+1):
        f=0
        w=0
        z=a
        if(check_Prime(a)):
            w=1
            a=str(a)
            p=list(a)
            for i in range(1,len(p)):
                a=len(p)-i-1
                sum=0
                for j in range(i,len(p)):
                    sum=sum+int(p[j])*pow(10,a)
                    a=a-1
                    if(check_Not_Prime(sum)):
                        f=1
        if(f==0 and w==1):
            print(z)
def right_Tru(p,r):
    for a in range(p,r+1):
        x=a
        f=0
        while(a>0):
            if(check_Not_Prime(a)):
                f=1
                break
            a=int(a/10)
        if f==0:
            print(x)
def Truncatable_Prime(Start_Limit,End_Limit,Truncatable_side):
    p,r=Start_Limit,End_Limit
    if(Truncatable_side=='l'):
        left_Tru(p,r)
    else:
        if(Truncatable_side=='r'):
            right_Tru(p,r)
        else:
            print("write Truncatable_side= l for left or r for right:")
