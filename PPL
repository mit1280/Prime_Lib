#1
def Balanced_Prime(Start_Limit,End_Limit,Balanced_Mode):
    p,r,c=Start_Limit,End_Limit,Balanced_Mode
    if p==1:
        p=2
    l=[]
    for a in range(p,r+1):
        k=0     
        for i in range(2,int(a/2)+1): 
            if(a%i==0):
                k=k+1     
                break
        if(k<=0):         
            l.append(a)
    for i in range(1,int(len(l)-1),1):
        m=l[i]
        p=i-1
        q=i+1
        sum=0
        z=c
        while(p>=0 and z>0):
            sum=sum+l[p]+l[q]
            q+=1
            p-=1
            x=q-p-2
            x=sum/x
            z=z-1
            if(x-int(x)==0):
                if(x==m):
                    print(m)
                    break
            if(q>len(l)-1):
                break
#2
def circular(x):
    u=list(map(int, str(x)))
    f=0
    for i in range(len(u)-1):
        a=u[0]
        u.remove(u[0])
        u.append(a)
        sum=0
        k=0
        j=len(u)-1
        while(j>=0):
            sum=sum+u[k]*(10**j)
            j-=1
            k+=1
        a=sum
        for i in range(2,int(a/2)+1): 
            if(a%i==0):
                f=1    
                break
        if f==1:
            break
    if f==0:
        print(x)
def Circular_Prime(Start_Limit,End_Limit):
    p,r=Start_Limit,End_Limit
    if p==1:
        p=2
    for a in range(p,r+1):
        k=0     
        for i in range(2,int(a/2)+1): 
            if(a%i==0):
                k=k+1     
                break
        if(k<=0):         
            circular(a)
#3
def Cousin_Prime(Start_Limit,End_Limit):
    p,r=Start_Limit,End_Limit
    if p==1:
        p=2
    x=''
    y=''
    #this is exception so we have to take explicitly
    if p<4 and r>6:
        print('( 3 , 7 )')
    for a in range(p,r+1): 
        k=0     
        for i in range(2,int(a/2)+1): 
            if(a%i==0):
                k=k+1     
                break
        if(k<=0):  
            if(x==''):
                x=int(a)
                continue
            if(y==''):
                y=int(a)
                if((y-x)==4):
                    print('(',x,',',y,')')
            else:
                x=y
                y=a
                if((y-x)==4):
                    print('(',x,',',y,')')
#4
def DoubleMersenne_Prime(Start_Limit,End_Limit):
    p,r=Start_Limit,End_Limit
    import  math
    if p==1:
        p=2
    for a in range(p,r+1):
        k=0     
        for i in range(2,int(a/2)+1): 
            if(a%i==0):
                k=k+1     
                break
        if(k<=0):  
            a+=1
            x=math.log2(a)
            z=x-int(x)
            if z==0:
                x+=1
                x=math.log2(x)
                z=x-int(x)
                if z==0:
                    print(a-1)
#5
def c_fact(n):
    i=2
    if n==0:
        return 0
    while int(n)>1:
        n=n/i
        i+=1
    z=n-int(n)
    if(z==0):
        return 1
    return 0
def Factorial_Prime(Start_Limit,End_Limit):
    p,r=Start_Limit,End_Limit
    if p==1:
        p=2
    for a in range(p,r+1):
        k=0     
        for i in range(2,int(a/2)+1): 
            if(a%i==0):
                k=k+1     
                break
        if(k<=0):  
            x=a-1
            y=a+1
            x=c_fact(x)
            if x==1:
                print (a)
            else:
                if 1==(c_fact(y)):
                    print(a)
#6
def Good_Prime(Start_Limit,End_Limit):
    s,r=Start_Limit,End_Limit
    q=[]
    b=0
    p=2
    for a in range(p,r+1):
        k=0     
        for i in range(2,int(a/2)+1): 
            if(a%i==0):
                k=k+1     
                break
        if(k<=0):  
            q.append(a)
            b=b+1
    a=r+1
    c=b
    while(b>0):
        k=0     
        for i in range(2,int(a/2)+1): 
            if(a%i==0):
                k=k+1     
                break
        if(k<=0):  
            q.append(a)
            b=b-1
        a=a+1
    while(c>0):
        if b>0:
            x=b
            f=0
            while(x>0):
                if(q[b]**2)<(q[b-x]*q[b+x]):
                    f=1
                x=x-1
            if f==0:
                if q[b]>s:
                    print(q[b])
        b=b+1
        c=c-1
