def Mersenne_Prime(Start_Limit,End_Limit):
    p,r=Start_Limit,End_Limit
    import  math
    if p==1:
        p=2
    for a in range(p,r+1):
        k=0     
        for i in range(2,int(a/2)+1): 
            if(a%i==0):
                k=k+1     
                break
        if(k<=0):  
            a+=1
            x=math.log2(a)
            x=x-int(x)
            if x==0:
                print(a-1)
#8
def Palindromic_Prime(Start_Limit,End_Limit):
    x,y=Start_Limit,End_Limit
    if x==1:
        x=2
    for a in range(x,y+1):
        k=0     
        for i in range(2,int(a/2)+1): 
            if(a%i==0):
                k=k+1     
                break
        if(k<=0):         
            sum=0
            num=a
            q=0
            p=[]
            while(num>0):
                rem=num%10
                p.append(rem)
                num=int(num/10)
                q+=1
                m=0
                while(q>0):
                    sum=sum+p[m]*(10**q)
                    m+=1
                    q-=1
                    sum/=10
                    if(a==sum):
                        print(a)
#9
def permutable(n):
    u=list(map(int, str(n)))
    f=0
    import itertools
    p1=list(itertools.permutations(u))
    for i in range(1,len(p1)):
        j=len(p1[0])-1
        sum=0
        k=0
        while(j>=0):
            sum=sum+p1[i][k]*(10**j)
            j-=1
            k+=1
        a=sum
        for i in range(2,int(a/2)+1): 
            if(a%i==0):
                f=1    
                break
        if f==1:
            break
    if f==0:
        print(n)
def Permutable_Prime(Start_Limit,End_Limit):
    p,r=Start_Limit,End_Limit
    if p==1:
        p=2
    for a in range(p,r+1):
        k=0     
        for i in range(2,int(a/2)+1): 
            if(a%i==0):
                k=k+1     
                break
        if(k<=0):         
            permutable(a)
#10
def check_Primorial(a):
    if(a<8):
        return 1
    p=a/2
    
    if(p!=int(p)):
        return 0
    while(p>0):
        for i in range(3,int(a/2)+1):
            f=0
            for j in range(2,i):
                if(i%j==0):
                    f=1
                    break
            if(f==0):
                
                p=p/i
                if(p!=int(p)):
                    p=0
                    break
                if(p==1):
                    return 1
    return 0            
def Primorial_Prime(Start_Limit,End_Limit):
    p,q=Start_Limit,End_Limit
    if(p==1):
        p=2
    for i in range(p,q):
        if(check_Prime(i)):
            x=0
            x=check_Primorial(i-1)
            if x==1:
                print(i)
            else:
               x=check_Primorial(i+1)
               if x==1:
                   print(i)
